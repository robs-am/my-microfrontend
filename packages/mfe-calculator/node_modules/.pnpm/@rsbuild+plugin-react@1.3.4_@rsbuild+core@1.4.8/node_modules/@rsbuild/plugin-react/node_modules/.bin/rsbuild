#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules/@rsbuild/core/bin/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules/@rsbuild/core/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules/@rsbuild/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules/@rsbuild/core/bin/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules/@rsbuild/core/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules/@rsbuild/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/@rsbuild+core@1.4.8/node_modules:/Users/roberta_amaro/Documents/Pessoal/my-microfrontend/packages/mfe-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@rsbuild+core@1.4.8/node_modules/@rsbuild/core/bin/rsbuild.js" "$@"
else
  exec node  "$basedir/../../../../../../@rsbuild+core@1.4.8/node_modules/@rsbuild/core/bin/rsbuild.js" "$@"
fi
